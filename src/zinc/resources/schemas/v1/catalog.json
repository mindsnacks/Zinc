{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://mindsnacks.github.io/Zinc/schemas/v1/catalog.json#",
	"type": "object",
	"required": ["id", "format"],
	"properties": {
		"id": {
			"type": "string",
			"minLength": 1
		},
		"format": {
			"type": "string",
			"pattern": "1"
		},
		"bundles": {
			"type": "object",
			"additionalProperties": false,
			"patternProperties": {
				".+": {
					"type": "object",
					"properties": {
						"distributions": {
							"type": "object",
							"additionalProperties": false,
							"patternProperties": {
								".+": {
									"type": "integer"
								}
							}
						}
					}
				}
			}
		},
		"next_version": {
			"type": "integer"
		},
		"versions": {
			"type": "array",
			"items": {
				"type": "integer",
				"minimum": 1
			}
		}
	}
}